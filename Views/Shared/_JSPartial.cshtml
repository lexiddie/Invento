<environment names="Development">
    
    
    <!-- The core Firebase JS SDK is always required and must be listed first -->
    <script src="/__/firebase/7.3.0/firebase-app.js"></script>
    
    <!-- TODO: Add SDKs for Firebase products that you want to use
         https://firebase.google.com/docs/web/setup#available-libraries -->
    <script src="/__/firebase/7.3.0/firebase-analytics.js"></script>
    
    <!-- Initialize Firebase -->
    <script src="/__/firebase/init.js"></script>

    <script type="text/javascript" src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script type="text/javascript" src="~/lib/jquery/dist/jquery-ui.min.js" asp-append-version="true"></script>
    <script type="text/javascript" src="~/lib/typeahead/typeahead.bundle.min.js" asp-append-version="true"></script>
    <script type="text/javascript" src="~/lib/bootstrap/js/bootstrap.min.js" asp-append-version="true"></script>
    <script type="text/javascript" src="~/lib/bootbox/bootbox.min.js" asp-append-version="true"></script>
    <script type="text/javascript" src="~/js/main.js" asp-append-version="true"></script>

    @* Data Tables *@
    <script type="text/javascript" src="~/lib/datatable/js/dataTables.min.js" asp-append-version="true"></script>
    <script type="text/javascript" src="~/lib/datatable/js/dataTables.bootstrap4.min.js" asp-append-version="true"></script>
    <script type="text/javascript" src="~/lib/datatable/js/responsive.bootstrap4.min.js" asp-append-version="true"></script>

</environment>          