                </div>
                <div>
                    @* <label>@($"{JsonConvert.DeserializeObject<List<BuildingCompletion>>(Model.Buildings).Count:n0}") Buildings</label> *@
                </div>
            </div>
            <div>
                <div>
                    <img src="@Url.Content("~/assets/icon/leftover.png")" alt="Category Logo"/>
                    <label>Leftover</label>
                </div>
                <div>
                    @* <label>@($"{JsonConvert.DeserializeObject<List<RoomCompletion>>(Model.Rooms).Count:n0}") Rooms</label> *@
                </div>
            </div>
        </div>  
        
        <div class="page-selection m-t-30">
            <div>    
                <img class="m-x-15" src="@Url.Content("~/assets/icon/visual.png")" alt="visualization Logo"/>
                <label class="m-l-10 m-t-5">Visualization</label>
            </div>    
            <div>    
                <label class="m-x-10">Semester</label>
                <label for="semester"></label>
