                </div>
            </div>
            <div>
                <div>
                    <img src="@Url.Content("~/assets/icon/product.png")" alt="Product Logo"/>
                    <label>Product</label>
                </div>
                <div>
                    @* <label>@($"{JsonConvert.DeserializeObject<List<RoomCompletion>>(Model.Rooms).Count:n0}") Rooms</label> *@
                </div>
            </div>
        </div>
        
        
        <div class="page-visualization m-t-30">
            <div>
                <div>
                    <img src="@Url.Content("~/assets/icon/purchase.png")" alt="Purchase Logo"/>
                    <label>Purchase</label>
                </div>
                <div>
                    @* <label> @($"{JsonConvert.DeserializeObject<List<CampusCompletion>>(Model.Campuses).Count:n0}") Campuses</label> *@
